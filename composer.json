{
    "name": "adrum/inertia-phpstan",
    "description": "PHPStan extension to validate Inertia.js page existence on disk",
    "type": "phpstan-extension",
    "keywords": [
        "phpstan",
        "inertia",
        "laravel",
        "static-analysis"
    ],
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^2.1.11",
        "phpstan/phpdoc-parser": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Adrum\\Inertia\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adrum\\Inertia\\PHPStan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
